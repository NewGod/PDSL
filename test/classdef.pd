class Mass{ 
	name = kg;
	1kg = 1000g;
	1kg = 0.001t;
}
class Distance{ 
	name = m;
	1000m = 1km;
	1m = 10dm;
	1dm = 10cm;
	1cm = 1000mm;
}
class Time{
	name = s;
	60s = 1min;
	60min = 1h;
	24h = 1d;
	365d = 1y;
}
class Empty{
}
class Acceleration{
	relation = Distance / Time ** 2;
}
class Force {
	name = N;
	relation = Acceleration * Mass;
}

